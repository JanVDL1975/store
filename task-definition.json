{
    "containerDefinitions": [
        {
            "name": "store-container",
            "image": "IMAGE_PLACEHOLDER",
            "cpu": 0,
            "memory": 1048576,
            "memoryReservation": 524288,
            "portMappings": [
                {
                    "name": "store-container-8080-tcp",
                    "containerPort": 8080,
                    "hostPort": 0,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "DB_USERNAME",
                    "value": "DB_USERNAME_PLACEHOLDER"
                },
                {
                    "name": "DB_URL",
                    "value": "DB_URL_PLACEHOLDER"
                },
                {
                    "name": "DB_PASSWORD",
                    "value": "DB_PASSWORD_PLACEHOLDER"
                }
            ],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/store-task",
                    "awslogs-region": "REGION_PLACEHOLDER",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            }
        }
    ],
    "family": "store-task",
    "taskRoleArn": "ROLE_PLACEHOLDER",
    "executionRoleArn": "ROLE_PLACEHOLDER",
    "networkMode": "bridge",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "EC2"
    ],
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "tags": []
}